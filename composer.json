{
  "name": "uraymr/laravel-googledrive-ext",
  "description": "Laravel 12 Google Drive storage extension using Flysystem v3 adapter.",
  "type": "library",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^12.0",
    "league/flysystem": "^3.0",
    "google/apiclient": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Uraymr\\GoogleDrive\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uraymr\\GoogleDrive\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Uraymr\\GoogleDrive\\GoogleDriveServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "authors": [
    {
      "name": "Uray Muhammad R.",
      "email": "umrajsyafiq@gmail.com"
    }
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true
}
